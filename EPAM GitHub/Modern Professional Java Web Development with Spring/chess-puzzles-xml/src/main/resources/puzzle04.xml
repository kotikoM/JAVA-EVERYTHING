    <bean id="blackQueen" class="com.epam.rd.autotasks.chesspuzzles.ChessPieceImpl">
        <constructor-arg value="A"/>
        <constructor-arg value="1"/>
        <constructor-arg value="Q"/>
    </bean>

    <bean id="pawn1" class="com.epam.rd.autotasks.chesspuzzles.ChessPieceImpl">
        <constructor-arg value="D"/>
        <constructor-arg value="4"/>
        <constructor-arg value="P"/>
    </bean>

    <bean id="pawn11" class="com.epam.rd.autotasks.chesspuzzles.ChessPieceImpl">
        <constructor-arg value="C"/>
        <constructor-arg value="4"/>
        <constructor-arg value="p"/>
    </bean>

    <bean id="pawn22" class="com.epam.rd.autotasks.chesspuzzles.ChessPieceImpl">
        <constructor-arg value="D"/>
        <constructor-arg value="3"/>
        <constructor-arg value="p"/>
    </bean>

    <bean id="whiteKing" class="com.epam.rd.autotasks.chesspuzzles.ChessPieceImpl">
        <constructor-arg value="B"/>
        <constructor-arg value="7"/>
        <constructor-arg value="k"/>
    </bean>

    <bean id="whiteBishop1" class="com.epam.rd.autotasks.chesspuzzles.ChessPieceImpl">
        <constructor-arg value="H"/>
